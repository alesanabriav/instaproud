{{#each []}}
<li class="list-group-item search-user-profile">
  <a href="#profile/{{ username }}">
    <span class="profile-image">
    {{#if profile_image }}
      <img src="images/{{ profile_image }}" class="pull-left">
      {{else}}
      <img src="images/placeholders/placeholder_profile.png">
      {{/if}}
    </span>

    <p>{{ username }}</p>
    <p>{{ name }}</p>
  </a>
</li>
{{/each}}
